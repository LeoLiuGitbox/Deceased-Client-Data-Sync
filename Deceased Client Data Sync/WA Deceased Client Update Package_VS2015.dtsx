          SQLTask:SqlStatementSource="--Data Cleaning--&#xA;&#xA;Delete From WA_Blacklist_Stage where F15 like '%U%' or F17 like '%U%'&#xA;Delete From WA_Blacklist_Stage where F2 like '%UNKNOW%' or F8 like '%UNKNOW%'&#xA;&#xA;&#xA;&#xA;Declare @archiveTableName varchar(50) &#xA;SET @archiveTableName = 'Z_BlackList_Archive' +  Convert(varchar(50), getdate(),120) + ''&#xA;EXEC sp_rename '[dbo].[BlackList]', @archiveTableName ;&#xA;&#xA;CREATE TABLE [dbo].[BlackList](&#xA;&#x9;[ID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[Full_Name] [nvarchar](200) NULL,&#xA;&#x9;[Sex] [nvarchar](2) NULL,&#x9;&#xA;&#x9;[DateofDeath] Datetime NULL,&#xA;&#x9;[DateofBirth] Datetime NULL,&#xA;&#x9;[Full_Address] [nvarchar](200) NULL&#xA;PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[ID] ASC&#xA;)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;Insert into BlackList&#xA;SELECT &#xA; upper([F8]+ ' ' + [F2]) [Full_Name]&#xA;,Replace([F1] ,'&quot;','')    [Sex]   &#xA;,Convert(datetime,dbo.SplitIndex('/',substring([F15],1,10) , 3 ) + '-' + dbo.SplitIndex('/',substring([F15],1,10) , 2) + '-' +  dbo.SplitIndex('/',substring([F15],1,10) , 1 ) ) [DateofDeath]&#xA;,Convert(datetime,dbo.SplitIndex('/',substring([F17],1,10) , 3 ) + '-' + dbo.SplitIndex('/',substring([F17],1,10) , 2) + '-' +  dbo.SplitIndex('/',substring([F17],1,10) , 1 ) )  [DateofBirth]      &#xA;,REPLACE(UPPER(ISNULL([F19],'')  + ' ' + ISNULL([F20],'') + ' ' +  ISNULL([F21],'') + ' ' + ISNULL([F22],'')  + ' ' + ISNULL([F23],'')) ,'WESTERN AUSTRALIA','WA' )   [Full_Address]&#xA;FROM [ExternalDataSorce].[dbo].[WA_Blacklist_Stage]&#xA;&#xA;INSERT INTO [dbo].[MatchList_Archive]&#xA;SELECT [Full_Name]&#xA;      ,[Sex]&#xA;      ,[DateofBirth]&#xA;      ,[DateofDeath]&#xA;      ,[Full_Address]&#xA;      ,[_Confidence]&#xA;      ,[_Similarity]&#xA;      ,[ComCarePerson_ID]&#xA;      ,[ComCareFull_Name]&#xA;      ,[ComCareSex]&#xA;      ,[ComCareDateofBirth]&#xA;      ,[ComCareDateofDeath]&#xA;      ,[ComCareFull_Address]&#xA;      ,[_Full_Name_Similarity]&#xA;      ,[_Full_Address_Similarity]&#xA;  FROM [ExternalDataSorce].[dbo].[MatchList_C1]&#xA;UNION ALL &#xA;SELECT [Full_Name]&#xA;      ,[Sex]&#xA;      ,[DateofBirth]&#xA;      ,[DateofDeath]&#xA;      ,[Full_Address]&#xA;      ,[_Confidence]&#xA;      ,[_Similarity]&#xA;      ,[ComCarePerson_ID]&#xA;      ,[ComCareFull_Name]&#xA;      ,[ComCareSex]&#xA;      ,[ComCareDateofBirth]&#xA;      ,[ComCareDateofDeath]&#xA;      ,[ComCareFull_Address]&#xA;      ,[_Full_Name_Similarity]&#xA;      ,[_Full_Address_Similarity]&#xA;  FROM [ExternalDataSorce].[dbo].[MatchList_C2]&#xA;UNION ALL &#xA;SELECT [Full_Name]&#xA;      ,[Sex]&#xA;      ,[DateofBirth]&#xA;      ,[DateofDeath]&#xA;      ,[Full_Address]&#xA;      ,[_Confidence]&#xA;      ,[_Similarity]&#xA;      ,[ComCarePerson_ID]&#xA;      ,[ComCareFull_Name]&#xA;      ,[ComCareSex]&#xA;      ,[ComCareDateofBirth]&#xA;      ,[ComCareDateofDeath]&#xA;      ,[ComCareFull_Address]&#xA;      ,[_Full_Name_Similarity]&#xA;      ,[_Full_Address_Similarity]&#xA;  FROM [ExternalDataSorce].[dbo].[MatchList_C3]&#xA;&#xA;DELETE FROM [dbo].[MatchList_C1]&#xA;DELETE FROM [dbo].[MatchList_C2]&#xA;DELETE FROM [dbo].[MatchList_C3]" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
