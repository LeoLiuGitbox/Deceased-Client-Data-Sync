              refId="Package\Re-create CC People List\CXDB_SP_Production"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="CXDB_SP_Production"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 'Dicharged' CC_Stat
	, Person.Person_ID CC_Person_ID
	, upper(Person.Given_Names + ' '+ Person.Last_Name) CC_Full_Name    
	, case (Person.gender_code) when 2 then 'F' when 1 then 'M' end  CC_Sex
	, convert(datetime, Person.Birth_Date, 103) CC_DateOfBirth
    , convert(datetime, deceased_date, 103) CC_DateOfDeceased
	,Ltrim(upper(Rtrim(isnull(ltrim(Location_Detail),''))+' '+ Rtrim(isnull(ltrim(building_name),'')+' '+Rtrim(isnull(ltrim(dwelling_number),''))+' '+Rtrim(isnull(ltrim(street_or_po_box),''))))
	+ ' ' + UPPER(REPLACE(GMAP_Address,', Australia',''))) CC_Address
	,NULL CC_Service_Type 
 		 
	from 
	SC_production.dbo.person Person	
	LEFT JOIN SC_production.dbo.client Client ON CLIENT.Client_ID = Person.Person_ID
	LEFT JOIN SC_production.dbo.period_of_residency res ON  Person.person_id=res.person_id  and res.display_indicator=1 
	LEFT JOIN SC_production.dbo.address ad ON Ad.address_id=res.address_id 
	LEFT JOIN SC_production.dbo.suburb sub ON ad.suburb_id=sub.suburb_id
		
	WHERE 	 Person.deceased_date is null
	and Client.Client_ID is not null
	and not exists (select 1 from SC_production.dbo.service_delivery where client_id = Person.person_id and to_date is null	)

	union all 

	select 'Current' CC_Stat
	, Person.Person_ID CC_Person_ID
	, upper(Person.Given_Names + ' '+ Person.Last_Name) CC_Full_Name   
	, case (Person.gender_code) when 2 then 'F' when 1 then 'M' end  CC_Sex
	, convert(datetime, Person.Birth_Date, 103) CC_DateOfBirth
    , convert(datetime, deceased_date, 103) CC_DateOfDeceased
	,Ltrim(upper(Rtrim(isnull(ltrim(Location_Detail),''))+' '+ Rtrim(isnull(ltrim(building_name),'')+' '+Rtrim(isnull(ltrim(dwelling_number),''))+' '+Rtrim(isnull(ltrim(street_or_po_box),''))))
	+ ' ' + UPPER(REPLACE(GMAP_Address,', Australia',''))) CC_Address
	,sertype.Description CC_Service_Type
	
	
 	from 
	SC_production.dbo.person Person	
	LEFT JOIN SC_production.dbo.client Client ON CLIENT.Client_ID = Person.Person_ID
	LEFT JOIN SC_production.dbo.period_of_residency res ON  Person.person_id=res.person_id and res.display_indicator=1 
	LEFT JOIN SC_production.dbo.address ad ON Ad.address_id=res.address_id 
	LEFT JOIN SC_production.dbo.suburb sub ON ad.suburb_id=sub.suburb_id
	CROSS APPLY
        (
        SELECT  TOP 1 service_type_code
        FROM    SC_production.dbo.service_delivery 
        WHERE   service_delivery.Client_ID =  Person.person_id 
		and to_date is null
		order by Creation_Date desc
        ) ser

	LEFT JOIN SC_production.dbo.service_type sertype on sertype.service_type_code = ser.service_type_code

	WHERE 	 Person.deceased_date is null
	and Client.Client_ID is not null
	and  exists (select 1 from SC_production.dbo.service_delivery
			where client_id = Person.person_id and to_date is null	)
	

	union all 

	select 'Donor' CC_Stat
	, Person.Person_ID CC_Person_ID
	, upper(Person.Given_Names + ' '+ Person.Last_Name) CC_Full_Name   
	, case (Person.gender_code) when 2 then 'F' when 1 then 'M' end  CC_Sex
	, convert(datetime, Person.Birth_Date, 103) CC_DateOfBirth
    , convert(datetime, deceased_date, 103) CC_DateOfDeceased
	,Ltrim(upper(Rtrim(isnull(ltrim(Location_Detail),''))+' '+ Rtrim(isnull(ltrim(building_name),'')+' '+Rtrim(isnull(ltrim(dwelling_number),''))+' '+Rtrim(isnull(ltrim(street_or_po_box),''))))
	+ ' ' + UPPER(REPLACE(GMAP_Address,', Australia',''))) CC_Address
	,NULL CC_Service_Type

 	from 
	SC_production.dbo.person Person	
	LEFT JOIN SC_production.dbo.client Client ON CLIENT.Client_ID = Person.Person_ID
	LEFT JOIN SC_production.dbo.donor_person Donor ON Donor.Donor_ID = Person.Person_ID
	LEFT JOIN SC_production.dbo.period_of_residency res ON  Person.person_id=res.person_id and res.display_indicator=1 
	LEFT JOIN SC_production.dbo.address ad ON Ad.address_id=res.address_id 
	LEFT JOIN SC_production.dbo.suburb sub ON ad.suburb_id=sub.suburb_id
	WHERE 	 
	Person.deceased_date is null
	and 
	Client.Client_ID is null
	and Donor.Donor_ID is not null</property>
